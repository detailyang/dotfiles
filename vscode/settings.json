{
    "workbench.colorTheme": "GitHub Plus",
    "http.proxy": "127.0.0.1:1087",
    "workbench.iconTheme": "Monokai Pro Icons",
    "go.buildOnSave": true,
    "go.lintOnSave": true,
    "go.vetOnSave": true,
    "go.buildFlags": [],
    "go.lintFlags": [],
    "go.vetFlags": [],
    "go.useCodeSnippetsOnFunctionSuggest": false,
    "go.formatOnSave": true,
    "go.formatTool": "goreturns",
    "go.goroot": "/usr/local/go",
    "go.gopath": "/Users/detailyang/go",
    "files.autoSave": "onFocusChange",
    "explorer.confirmDragAndDrop": false,
    "python.formatting.provider": "yapf",
    "editor.formatOnSave": true,
    "python.pythonPath": "/usr/bin/python",
    "window.zoomLevel": 0,
    "rust.mode": "rls",
    "workbench.activityBar.visible": true,
    "editor.fontSize": 13.5,
    "breadcrumbs.enabled": true,
    "editor.rulers": [
        80,
        120
    ],
    "vs-kubernetes": {
        "vs-kubernetes.helm-path": "/Users/detailyang/.vs-kubernetes/tools/helm/darwin-amd64/helm",
        "vs-kubernetes.draft-path": "/Users/detailyang/.vs-kubernetes/tools/draft/darwin-amd64/draft",
        "vs-kubernetes.minikube-path": "/Users/detailyang/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
    },
    "editor.tabSize": 4,
    "[javascript]": {
        "editor.tabSize": 2
    },
    "[json]": {
        "editor.tabSize": 2
    },
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "editor.minimap.enabled": false,
    "go.useLanguageServer": true,
    "go.languageServerExperimentalFeatures": {
        "diagnostics": true // for diagnostics as you type
    },
    "[go]": {
        "editor.snippetSuggestions": "none",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        },
    },
    "gopls": {
        "usePlaceholders": false, // add parameter placeholders when completing a function
        "enhancedHover": true, // experimental to improve quality of hover (will be on by default soon)
    },
    "go.coverOnSingleTest": true,
    "go.coverOnSave": true
}